{
  "name": "moon-climber",
  "version": "0.0.0",
  "description": "Stream graphics for a Moonshot Celeste race",
  "homepage": "",
  "author": {
    "name": "oneirocosm",
    "email": "software@oneirocosm.com",
    "url": ""
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "stream",
    "celeste",
    "nodecg",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^2.0.0",
    "dashboardPanels": [
      {
        "name": "controls",
        "title": "Controls",
        "width": 3,
        "file": "panel.html",
        "headerColor": "#040227"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "node scripts/build.mjs --all",
    "build:extension": "node scripts/build.mjs --extension",
    "watch": "node scripts/build.mjs --all --watch",
    "watch:browser": "node scripts/build.mjs --dashboard --graphics --watch",
    "watch:schemas": "node scripts/build.mjs --schemas --watch",
    "dev": "concurrently --kill-others \"npm run watch:schemas\" \"npm run watch:browser\" \"nodemon\"",
    "generate-schema-types": "trash src/types/schemas && nodecg schema-types"
  },
  "dependencies": {
    "@nodecg/react-hooks": "^1.0.3",
    "@pixi/react": "^7.1.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/url-parse": "^1.4.11",
    "pixi.js": "^7.2.4",
    "punycode": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8",
    "ts-node": "*",
    "url-parse": "^1.5.10",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@nodecg/types": "^2.0.0",
    "@parcel/config-default": "*",
    "@parcel/core": "*",
    "@parcel/reporter-cli": "*",
    "@parcel/validator-typescript": "*",
    "@types/node": "^18",
    "@types/ws": "^8.5.10",
    "concurrently": "*",
    "glob": "^10.2.7",
    "nodemon": "*",
    "trash-cli": "*",
    "typescript": "^5.1.3"
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT"
}
